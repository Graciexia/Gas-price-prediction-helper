<p id="notice"><%= notice %></p>

<h1>Listing Gas Grades</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @gas_grades.each do |gas_grade| %>
      <tr>
        <td><%= link_to 'Show', gas_grade %></td>
        <td><%= link_to 'Edit', edit_gas_grade_path(gas_grade) %></td>
        <td><%= link_to 'Destroy', gas_grade, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Gas grade', new_gas_grade_path %>
